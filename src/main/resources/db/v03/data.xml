<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

  <changeSet id="add_portal_settings" author="Daniil_Nasretdinov@epam.com">
    <insert tableName="SETTINGS">
      <column name="ID" value="OPEN_PORTAL_FOR_TICK" />
      <column name="VAL" value="0.10" />
      <column name="DESCRIPTION">Вероятность возникновения портала за тик</column>
    </insert>
    <insert tableName="SETTINGS">
      <column name="ID" value="PLANET_QUANTITY" />
      <column name="VAL" value="0.20" />
      <column name="DESCRIPTION">Вероятность возникновения портала от количества планет</column>
    </insert>
  </changeSet>
</databaseChangeLog>